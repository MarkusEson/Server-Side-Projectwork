@model IEnumerable<Service.Models.Administrator>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@{
    ViewBag.Title = "Administrator";
}

<h2>Administrators</h2>

<ul>
    @foreach (var admin in Model)
    {
        <li><a href="/Admin/Details/@admin.AdminId" class="btn btn-default" id="adminBtn">@admin.FirstName @admin.LastName</a></li>
    }

</ul>

@if (Session["UserName"] != null)
{
    <a href="/Admin/Create" class="btn btn-default">Create new admin!</a>
}