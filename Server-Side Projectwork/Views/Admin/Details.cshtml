@model Service.Models.Administrator
@{
    ViewBag.Title = "Details";
}

<div id="nameDiv">
   <a>@Model.FirstName @Model.LastName</a>
</div>

<br />
<br />

<div id="descDiv">
    <a id="desc">@Model.Description</a>
</div>

@if (Session["UserSession"] != null)
{
    if (Session["UserName"].Equals(Model.UserName))
    {
        <a href="/Admin/Edit/@Model.AdminId" style="margin-top: 10px;" class="btn btn-default">Edit</a>
        <a href="/Admin/Delete/@Model.AdminId" style="margin-top: 10px;" class="btn btn-default">Delete</a>
    }
}

<div>
    @Html.ActionLink("Back to List", "../Admin")
</div>